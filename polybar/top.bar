ws-icon-3 = 4;4
ws-icon-4 = 5;5
ws-icon-5 = 6;6
ws-icon-6 = 7;7
ws-icon-7 = 8;8

format = <label-state>

label-focused = %{B#0c977f F#0f2536}ÓÇº%{B- F-}%{B#0c977f F#b9fafc}%icon%%{B- F-}%{B#0c977f F#0f2536}ÓÇ∫%{B- F-}
