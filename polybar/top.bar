[colors]
bg = ${xrdb:color8:#0f2536}
fg = ${xrdb:color15:#b9fafc}

active = #0c977f
inactive  = #086757

black1= ${xrdb:color0:#071017}
red1 = ${xrdb:color1:#497B5A}
green1 = ${xrdb:color2:#25C66E}
yellow1 = ${xrdb:color3:#1C6565}
blue1 = ${xrdb:color4:#196D6D}
magenta1 = ${xrdb:color5:#1B5C62}
cyan1 = ${xrdb:color6:#148A76}
white1 = ${xrdb:color7:#87b6b7}

black2 = ${xrdb:color8:#0f2536}
red2 = ${xrdb:color9:#52ac70}
green2 = ${xrdb:color10:#20ff8d}
yellow2 = ${xrdb:color11:#1b8b8b}
blue2 = ${xrdb:color12:#179696}
magenta2 = ${xrdb:color13:#1a7e87}
cyan2 = ${xrdb:color14:#0fbda0}
white2 = ${xrdb:color15:#b9fafc}


[bar/topbar]
bottom = false
width = 100%
height = 18
fixed-center = true
background = ${colors.bg}
border-top-size = 5
border-right-size = 5
border-left-size = 5
border-bottom-size = 5
border-color = ${colors.black2}

line-size = 2

font-0 = "Source Code Pro:pixelsize=11;1"
font-1 = TerminessTTF Nerd Font Mono:pixelsize=16:antialias=true;4
font-2 = unifont:fontformat=truetype:size=8:antialias=true;0 ;BARS
font-3 = "Noto Sans CJK JP:size=7:antialias=false;2"
font-4 = Wuncon Siji:pixelsize=8:antialias=false;2

modules-left = lock
modules-center = i32
modules-right = powermenu


[module/temperature]
type = internal/temperature
thermal-zone = 0
warn-temperature = 60

format = <label>
format-warn = <ramp> <label-warn>
format-prefix = %{B#0fbda0 F#0f2536}ÓÇ∏%{B- F-}%{B#0fbda0 F#0f2536}%{B- F-}%{B#148A76 F#0fbda0}ÓÇ∏%{B- F-}
format-suffix = %{B#148A76 F#0f2536}ÓÇæ%{B- F-}

label-foreground = ${colors.fg}
label-background = ${colors.cyan1}
label = TMP %temperature-c%
label-warn = %temperature-c%
label-warn-foreground = ${colors.cyan1}

ramp-0 = ÓÉâ
ramp-1 = ÓÉä
ramp-2 = ÓÉã
ramp-foreground = ${colors.fg}


[module/cpu]
type = internal/cpu
interval = 2
format-prefix = %{B#0fbda0 F#0f2536}ÓÇ∏%{B- F-}%{B#0fbda0 F#0f2536}%{B- F-}%{B#148A76 F#0fbda0}ÓÇ∏%{B- F-}
format-suffix = %{B#148A76 F#0f2536}ÓÇæ%{B- F-}
label-foreground = ${colors.fg}
label-background = ${colors.cyan1}
label = CPU %percentage%%


[module/memory]
type = internal/memory
interval = 2
format-prefix = %{B#0fbda0 F#0f2536}ÓÇ∏%{B- F-}%{B#0fbda0 F#0f2536}ÓÇ™%{B- F-}%{B#148A76 F#0fbda0}ÓÇ∏%{B- F-}
format-suffix = %{B#148A76 F#0f2536}ÓÇæ%{B- F-}
label-foreground = ${colors.fg}
label-background = ${colors.cyan1}
label = RAM %percentage_used%%


[module/previous]
type = custom/script
interval = 86400
format = "%{T3}<label>"
format-padding = 0
exec = echo "ÔÅà"
line-size = 1
click-left = "dbus-send --print-reply --dest=org.mpris.MediaPlayer2.spotify /org/mpris/MediaPlayer2 org.mpris.MediaPlayer2.Player.Previous"

format-prefix = %{B#0c977f F#0f2536}ÓÇº%{B- F-}%{B#0c977f F#b9fafc}%{B- F-}%{B#0c977f F#0f2536}%{B- F-}
format-suffix = %{B#148A76 F#0f2536}ÓÇ∫%{B- F-}
label-foreground = ${colors.fg}
label-background = ${colors.cyan1}


[module/next]
type = custom/script
interval = 86400
format = "%{T3}<label>"
format-padding = 0
exec = echo "ÔÅë"
line-size = 1
click-left = "dbus-send --print-reply --dest=org.mpris.MediaPlayer2.spotify /org/mpris/MediaPlayer2 org.mpris.MediaPlayer2.Player.Next"
format-prefix = %{B#0c977f F#0f2536}ÓÇº%{B- F-}%{B#0c977f F#b9fafc}%{B- F-}%{B#0c977f F#0f2536}%{B- F-}
format-suffix = %{B#148A76 F#0f2536}ÓÇ∫%{B- F-}
label-foreground = ${colors.fg}
label-background = ${colors.cyan1}


[module/playpause]
type = custom/script
exec = if [[ "$(playerctl status)" = "Playing" ]]; then echo -n "‚òê"; else echo -n "ÔÅã"; fi
exec-if = [[ "$(playerctl status)" = "Playing" || "$(playerctl status)" = "Paused" ]] && echo "0"
interval = 1
format = <label>
click-left = "dbus-send --print-reply --dest=org.mpris.MediaPlayer2.spotify /org/mpris/MediaPlayer2 org.mpris.MediaPlayer2.Player.PlayPause"

format-prefix = %{B#0c977f F#0f2536}ÓÇº%{B- F-}%{B#0c977f F#b9fafc}%{B- F-}%{B#0c977f F#0f2536}%{B- F-}
format-suffix = %{B#148A76 F#0f2536}ÓÇ∫%{B- F-}
label-foreground = ${colors.fg}
label-background = ${colors.cyan1}


[module/lock]
type = custom/script
exec = echo "[üîí]"
interval = 1
format = <label>
click-left = /usr/bin/i3lock-fancy

format-prefix = %{B#0c977f F#0f2536}ÓÇº%{B- F-}%{B#0c977f F#b9fafc}%{B- F-}%{B#0c977f F#0f2536}%{B- F-}
format-suffix = %{B#148A76 F#0f2536}ÓÇ∫%{B- F-}
label-foreground = ${colors.fg}
label-background = ${colors.cyan1}


[module/spotify]
type = custom/script
interval = 1
format-prefix = " ÔÄÅ "
format = <label>
exec = python /home/zack/programming/programs/polybar-spotify/spotify_status.py -f '{song} - {artist}'
format-underline = #1db954


[module/powermenu]
type = custom/menu
expand-right = true
#format-spacing = 1
#format-underline = #1db954

format-prefix = %{B#0fbda0 F#0f2536}ÓÇ∏%{B- F-}%{B#0fbda0 F#0f2536}ÓÇ™%{B- F-}%{B#148A76 F#0fbda0}ÓÇ∏%{B- F-}
format-suffix = %{B#0fbda0 F#0f2536}ÓÇ∏%{B- F-}%{B#0fbda0 F#0f2536}ÓÇ™%{B- F-}%{B#148A76 F#0fbda0}%{B- F-}
label-foreground = ${colors.fg}
label-background = ${colors.cyan1}

label-open = power
#label-open-foreground = #81a2be
label-open-foreground = ${colors.fg}
label-close = ÓÅ• cancel
#label-close-foreground = #81a2be
label-close-foreground = ${colors.fg}
label-separator = |
#label-separator-foreground = #81a2be
label-separator-foreground = ${colors.fg}

menu-0-0 = reboot
menu-0-0-exec = menu-open-1
menu-0-1 = power off
menu-0-1-exec = menu-open-2

menu-1-0 = cancel
menu-1-0-exec = menu-open-0
menu-1-1 = reboot
menu-1-1-exec = reboot

menu-2-0 = power off
menu-2-0-exec = poweroff
#menu-2-1 = cancel
#menu-2-1-exec = menu-open-0


[module/wlan2]
type = internal/network
interface = enp0s31f6
interval = 1.0
udspeed-minwidth = 3

format-connected = <label-connected>
format-disconnected = <label-disconnected>
label-connected = %{A2:nm-connection-editor:}%downspeed%%{B#0fbda0 F#148A76}ÓÇ∏%{B- F-}%{B#0fbda0 F#0f2536}ÓÑ´%{B- F-}%{B#148A76 F#0fbda0}ÓÇ∏%{B- F-}%{B#148A76 F#b9fafc}%upspeed%%{B- F-}%{A}
label-disconnected = ‚Äàdisconnected‚Äà
format-connected-prefix = %{B#0fbda0 F#0f2536}ÓÇ∏%{B- F-}%{B#0fbda0 F#0f2536}ÓÑ¨%{B- F-}%{B#148A76 F#0fbda0}ÓÇ∏%{B- F-}
format-connected-suffix = %{B#148A76 F#0f2536}ÓÇæ%{B- F-}
format-disconnected-prefix = %{B#0fbda0 F#0f2536}ÓÇ∏%{B- F-}%{B#0fbda0 F#0f2536}ÓÅÜ%{B- F-}%{B#148A76 F#0fbda0}ÓÇ∏%{B- F-}
format-disconnected-suffix = %{B#148A76 F#0f2536}ÓÇæ%{B- F-}
label-connected-foreground = ${colors.fg}
label-connected-background = ${colors.cyan1}
label-disconnected-foreground = ${colors.fg}
label-disconnected-background = ${colors.cyan1}


[module/date2]
type = internal/date
interval = 1
label = %{A3:gnome-calculator:}%date%%time%%{A}
time = %I:%M %p
time-alt = %H:%M:%S
date-alt = "%a %d.%m.%Y "
format-underline = #1db954
#format-prefix = %{B#52ac70 F#0f2536}ÓÇ∏%{B- F-}%{B#52ac70 F#0f2536}ÓÄñ%{B- F-}%{B#497B5A F#52ac70}ÓÇ∏%{B- F-}
#format-suffix = %{B#497B5A F#0f2536}ÓÇæ%{B- F-}


[module/audio2]
type = internal/pulseaudio
sink = alsa_output.pci-0000_00_1f.3.analog-stereo
format-volume = %{A2:pavucontrol:}<label-volume>%{A}
label-volume = VOL %percentage:2%%
label-muted = MUTE
format-volume-prefix = %{B#0fbda0 F#0f2536}ÓÇ∏%{B- F-}%{B#0fbda0 F#0f2536}ÓÇ™%{B- F-}%{B#148A76 F#0fbda0}ÓÇ∏%{B- F-}
format-volume-suffix = %{B#148A76 F#0f2536}ÓÇæ%{B- F-}
format-muted-prefix = %{B#0fbda0 F#0f2536}ÓÇ∏%{B- F-}%{B#0fbda0 F#0f2536}ÓÇ™%{B- F-}%{B#148A76 F#0fbda0}ÓÇ∏%{B- F-}
format-muted-suffix = %{B#148A76 F#0f2536}ÓÇæ%{B- F-}

label-volume-foreground = ${colors.fg}
label-volume-background = ${colors.cyan1}
label-muted-foreground = ${colors.fg}
label-muted-background = ${colors.cyan1}
label = VOL


[module/i32]
type = internal/i3
pin-workspaces = true
