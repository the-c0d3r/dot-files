            "complex_modifications": {
                "rules": [
                    {
                        "description": "Replace window switch with yabai",
                        "enabled": true,
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "left_arrow",
                                    "modifiers": {
                                        "mandatory": [
                                            "control"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "shell_command": "/etc/profiles/per-user/thuyein/bin/yabai -m space --focus prev"
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "right_arrow",
                                    "modifiers": {
                                        "mandatory": [
                                            "control"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "shell_command": "/etc/profiles/per-user/thuyein/bin/yabai -m space --focus next"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Ctrl+Left Click => Cmd+Left Click",
                        "enabled": true,
                        "manipulators": [
                            {
                                "from": {
                                    "modifiers": {
                                        "mandatory": [
                                            "control"
                                        ],
                                        "optional": [
                                            "any"
                                        ]
                                    },
                                    "pointing_button": "button1"
                                },
                                "to": [
                                    {
                                        "modifiers": [
                                            "command"
                                        ],
                                        "pointing_button": "button1"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
